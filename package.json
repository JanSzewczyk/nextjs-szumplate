{
  "name": "nextjs-szumplate",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "analyze": "ANALYZE=true npm run build",
    "build": "next build",
    "dev": "next dev --turbopack",
    "e2e": "playwright test --pass-with-no-tests",
    "e2e:ci": "CI=true playwright test --pass-with-no-tests",
    "e2e:ui": "playwright test --ui --pass-with-no-tests",
    "lint": "next lint",
    "lint:ci": "next lint . -o eslint-results.sarif -f @microsoft/eslint-formatter-sarif",
    "lint:fix": "next lint --fix",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "start": "next start",
    "storybook:build": "storybook build",
    "storybook:dev": "storybook dev -p 6006",
    "storybook:serve": "serve storybook-static",
    "test": "vitest run --passWithNoTests",
    "test:ci": "CI=true vitest --passWithNoTests",
    "test:coverage": "vitest --coverage --passWithNoTests",
    "test:storybook": "vitest --project=storybook --coverage --passWithNoTests",
    "test:ui": "vitest --ui",
    "test:unit": "vitest --project=unit --coverage --passWithNoTests",
    "test:watch": "vitest watch --passWithNoTests",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^15.3.1",
    "@szum-tech/design-system": "^2.1.0",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@tailwindcss/postcss": "^4.1.4",
    "next": "^15.3.1",
    "next-compose-plugins": "^2.2.1",
    "postcss": "^8.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.0",
    "server-only": "^0.0.1",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.6",
    "@playwright/test": "^1.52.0",
    "@storybook/addon-a11y": "^8.6.12",
    "@storybook/addon-docs": "^8.6.12",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-links": "^8.6.12",
    "@storybook/experimental-addon-test": "^8.6.12",
    "@storybook/experimental-nextjs-vite": "^8.6.12",
    "@storybook/react": "^8.6.12",
    "@storybook/theming": "^8.6.12",
    "@szum-tech/eslint-config": "^2.1.6",
    "@szum-tech/prettier-config": "^1.6.1",
    "@szum-tech/semantic-release-config": "^2.3.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/browser": "^3.1.1",
    "@vitest/coverage-v8": "^3.1.1",
    "@vitest/ui": "^3.1.1",
    "eslint": "^9.25.0",
    "playwright": "^1.51.1",
    "prettier": "^3.5.3",
    "semantic-release": "^24.2.3",
    "serve": "^14.2.4",
    "storybook": "^8.6.12",
    "storybook-dark-mode": "^4.0.2",
    "tailwindcss": "^4.1.3",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.1"
  }
}
