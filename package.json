{
  "name": "nextjs-szumplate",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "analyze": "cross-env ANALYZE=true npm run build",
    "build": "next build",
    "dev": "next dev",
    "e2e": "playwright test --pass-with-no-tests",
    "e2e:ci": "cross-env CI=true playwright test --pass-with-no-tests",
    "e2e:ui": "playwright test --ui --pass-with-no-tests",
    "lint": "next lint",
    "lint:ci": "next lint --max-warnings=0",
    "lint:fix": "next lint --fix",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "start": "next start",
    "test": "vitest run --passWithNoTests",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch --passWithNoTests",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^13.4.19",
    "@szum-tech/design-system": "^1.13.1",
    "@t3-oss/env-nextjs": "^0.6.1",
    "next": "^13.4.19",
    "next-compose-plugins": "^2.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "server-only": "^0.0.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.37.1",
    "@szum-tech/prettier-config": "^1.3.1",
    "@szum-tech/semantic-release-preset": "^1.5.2",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^6.1.2",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/node": "^20.5.7",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/semantic-release": "^20.0.1",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.5.0",
    "@vitejs/plugin-react": "^4.0.4",
    "@vitest/coverage-v8": "^0.34.3",
    "autoprefixer": "^10.4.15",
    "cross-env": "^7.0.3",
    "eslint": "^8.48.0",
    "eslint-config-next": "^13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-playwright": "^0.16.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "eslint-plugin-testing-library": "^6.0.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "eslint-plugin-vitest": "^0.2.8",
    "happy-dom": "^10.11.2",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "semantic-release": "^21.1.1",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.34.3"
  }
