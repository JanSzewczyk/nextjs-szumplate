{
  "name": "nextjs-szumplate",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "analyze": "cross-env ANALYZE=true npm run build",
    "build": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "lint:ci": "next lint --max-warnings=0",
    "lint:fix": "next lint --fix",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "start": "next start",
    "test": "vitest run --passWithNoTests",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test --pass-with-no-tests",
    "test:e2e:ci": "cross-env CI=true playwright test --pass-with-no-tests",
    "test:e2e:ui": "playwright test --ui --pass-with-no-tests",
    "test:watch": "vitest watch --passWithNoTests",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^13.4.16",
    "@szum-tech/design-system": "^1.12.3",
    "@t3-oss/env-nextjs": "^0.6.0",
    "next": "13.4.16",
    "next-compose-plugins": "^2.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "zod": "^3.22.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.37.0",
    "@szum-tech/prettier-config": "^1.3.0",
    "@szum-tech/semantic-release-preset": "^1.5.2",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/node": "^20.5.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/semantic-release": "^20.0.1",
    "@vitejs/plugin-react": "^4.0.4",
    "@vitest/coverage-v8": "^0.34.2",
    "autoprefixer": "^10.4.15",
    "cross-env": "^7.0.3",
    "eslint": "^8.47.0",
    "eslint-config-next": "^13.4.16",
    "happy-dom": "^10.10.0",
    "postcss": "^8.4.28",
    "prettier": "^3.0.2",
    "semantic-release": "^21.0.7",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.34.2"
  }
}
