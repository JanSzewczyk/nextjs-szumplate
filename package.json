{
  "name": "nextjs-szumplate",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "analyze": "cross-env ANALYZE=true npm run build",
    "build": "next build",
    "dev": "next dev",
    "e2e": "playwright test --pass-with-no-tests",
    "e2e:ci": "cross-env CI=true playwright test --pass-with-no-tests",
    "e2e:ui": "playwright test --ui --pass-with-no-tests",
    "lint": "next lint",
    "lint:ci": "next lint --max-warnings=0",
    "lint:fix": "next lint --fix",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "start": "next start",
    "test": "vitest run --passWithNoTests",
    "test:ci": "cross-env CI=true vitest run --passWithNoTests",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui --passWithNoTests",
    "test:watch": "vitest watch --passWithNoTests",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^14.2.4",
    "@szum-tech/design-system": "^1.18.3",
    "@t3-oss/env-nextjs": "^0.10.1",
    "next": "^14.2.4",
    "next-compose-plugins": "^2.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "server-only": "^0.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.44.1",
    "@szum-tech/prettier-config": "^1.3.4",
    "@szum-tech/semantic-release-preset": "^1.5.6",
    "@testing-library/dom": "^10.1.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/semantic-release": "^20.0.6",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "eslint": "^9.5.0",
    "eslint-config-next": "^14.2.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-tailwindcss": "^3.17.3",
    "eslint-plugin-testing-library": "^6.2.2",
    "eslint-plugin-unused-imports": "^4.0.0",
    "happy-dom": "^14.11.1",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "semantic-release": "^23.1.1",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  }
}
