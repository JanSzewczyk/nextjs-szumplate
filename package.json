{
  "name": "nextjs-szumplate",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "analyze": "ANALYZE=true npm run build",
    "build": "next build",
    "dev": "next dev",
    "lint": "eslint .",
    "lint:ci": "eslint . -o eslint-results.sarif -f @microsoft/eslint-formatter-sarif",
    "lint:fix": "eslint --fix .",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "start": "next start",
    "storybook:build": "storybook build",
    "storybook:dev": "storybook dev -p 6006",
    "storybook:serve": "serve storybook-static",
    "test": "vitest run --passWithNoTests",
    "test:ci": "CI=true vitest --coverage",
    "test:coverage": "vitest --coverage --passWithNoTests",
    "test:e2e": "playwright test --pass-with-no-tests",
    "test:e2e:ci": "CI=true playwright test --pass-with-no-tests",
    "test:e2e:ui": "playwright test --ui --pass-with-no-tests",
    "test:storybook": "vitest --project=storybook --coverage --passWithNoTests",
    "test:ui": "vitest --ui",
    "test:unit": "vitest --project=unit --coverage --passWithNoTests",
    "test:watch": "vitest watch --passWithNoTests",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^16.1.4",
    "@szum-tech/design-system": "^3.11.1",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@tailwindcss/postcss": "^4.1.14",
    "babel-plugin-react-compiler": "^1.0.0",
    "lucide-react": "^0.563.0",
    "next": "^16.1.4",
    "next-compose-plugins": "^2.2.1",
    "next-themes": "^0.4.6",
    "pino": "^10.3.0",
    "pino-pretty": "^13.1.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.71.1",
    "server-only": "^0.0.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^5.0.0",
    "@playwright/test": "^1.58.0",
    "@storybook-community/storybook-dark-mode": "^7.1.0",
    "@storybook/addon-a11y": "^10.2.0",
    "@storybook/addon-docs": "^10.2.0",
    "@storybook/addon-vitest": "^10.2.0",
    "@storybook/nextjs-vite": "^10.2.0",
    "@szum-tech/eslint-config": "^2.1.15",
    "@szum-tech/prettier-config": "^1.6.2",
    "@szum-tech/semantic-release-config": "^2.3.7",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/node": "^25.0.10",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.1",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.16",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.4",
    "playwright": "^1.56.0",
    "prettier": "^3.8.1",
    "semantic-release": "^25.0.0",
    "serve": "^14.2.5",
    "storybook": "^10.1.11",
    "storybook-addon-tag-badges": "^3.0.4",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^6.0.5",
    "vitest": "^4.0.16"
  }
}
